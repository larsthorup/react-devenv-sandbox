{
  "name": "react-devenv-sandbox",
  "version": "0.1.0",
  "type": "module",
  "author": "Lars Thorup <lars@zealake.com> (https://www.zealake.com/)",
  "license": "MIT",
  "dependencies": {
    "history": "^5.0.0",
    "path-to-regexp": "^6.1.0",
    "query-string": "^6.13.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "@testing-library/react": "^11.0.2",
    "@types/chai": "^4.2.12",
    "@types/history": "^4.7.7",
    "@types/http-server": "^0.10.0",
    "@types/http-terminator": "^2.0.1",
    "@types/mocha": "^8.0.1",
    "@types/node": "^14.0.26",
    "@types/puppeteer-core": "^2.0.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "chai": "^4.2.0",
    "chrome-launcher": "^0.13.4",
    "coveralls": "^3.1.0",
    "eslint": "^7.5.0",
    "http-server": "^0.12.3",
    "http-terminator": "^2.0.3",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.5",
    "puppeteer-core": "^5.2.1",
    "puppeteer-to-istanbul": "^1.4.0",
    "rollup": "^2.23.0",
    "sinon": "^9.0.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "node dev/asset-bundle.cjs && npm run rollup && npm run postcss",
    "coveralls": "coveralls < coverage/lcov.info",
    "eslint": "eslint --cache .",
    "nyc-report": "nyc report --reporter=html --reporter=text-summary --reporter=lcov",
    "postcss": "postcss --use postcss-import --output build/app/index.css app/index.css",
    "prettier": "prettier --write --ignore-path .gitignore .",
    "puppeteer": "node dev/puppeteer-runner",
    "rollup": "rollup --config rollup.config.js",
    "start": "http-server . -o",
    "start:build": "npm run build && http-server build -o",
    "test": "npm run prettier && npm run eslint && npm run tsc && npm run puppeteer && npm run nyc-report && npm run build",
    "travis": "npm test && npm run coveralls",
    "tsc": "tsc"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "extends": "eslint:recommended",
    "ignorePatterns": [
      "build"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "root": true
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
